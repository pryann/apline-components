{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "outDir": "./public/js",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true
  },
  "include": ["src/**/*", "src/index.js", "src/components/usersTable/usersTable.js", "src/utils/pagination.js", "src/utils/sorting.js"],
  "exclude": ["node_modules"]
}
